  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/jest": "^24.0.12",
    "@types/uuid": "^3.4.4",
    "husky": "^2.2.0",
    "lerna": "^3.0.5",
    "mobx": "^5.9.4",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "shx": "^0.3.2",
    "tsdx": "^0.7.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "immer": "^3.1.1",
    "ts-essentials": "^2.0.7",
    "uuid": "^3.3.2"
  },
  "directories": {
    "test": "test"
  }
