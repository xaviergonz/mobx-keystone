{
  "name": "mobx-state-tree-next",
  "version": "0.1.0",
  "description": "Opinionated, transactional, MobX powered state container",
  "keywords": [
    "mobx",
    "mobx-state-tree-next",
    "mobx-state-tree",
    "reactive",
    "frp",
    "functional-reactive-programming",
    "state management"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "umd:main": "dist/mobx-state-tree-next.umd.production.js",
  "module": "dist/mobx-state-tree-next.es.production.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "build-docs": "shx rm -rf ../site/public/api && typedoc --options ./typedocconfig.js"
  },
  "peerDependencies": {
    "mobx": "^5.9.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
