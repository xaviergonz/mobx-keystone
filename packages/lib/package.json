{
  "name": "mobx-data-model",
  "version": "0.1.0",
  "description": "A MobX powered state management solution based on data trees with first class support for Typescript, snapshots, patches and much more",
  "keywords": [
    "mobx",
    "mobx-data-model",
    "mobx-state-tree",
    "reactive",
    "frp",
    "functional-reactive-programming",
    "state management"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "umd:main": "dist/mobx-data-model.umd.production.js",
  "module": "dist/mobx-data-model.es.production.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "shx cp ../../README.md . && tsdx build",
    "test": "tsdx test",
    "test:perf": "yarn build && yarn test:perf:run",
    "test:perf:run": "cd perf_test && export NODE_ENV=production && /usr/bin/time ts-node ./report.ts",
    "build-docs": "shx rm -rf ../site/src/public/api && typedoc --options ./typedocconfig.js"
  },
  "peerDependencies": {
    "mobx": "^5.9.4"
  },
  "devDependencies": {
    "@types/jest": "^24.0.12",
    "@types/nanoid": "^2.0.0",
    "mobx": "^5.9.4",
    "shx": "^0.3.2",
    "ts-node": "^8.3.0",
    "tsdx": "^0.7.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "nanoid": "^2.0.3",
    "ts-essentials": "^2.0.7"
  },
  "directories": {
    "test": "test"
  }
}
